
@{
    ViewBag.Title = "Error";
}

<div class="row">
    <div class="col-md-12">
        <div class="error-template success">
            <h1>
                Oops!
            </h1>            
            <div class="error-details">
                Sorry, an error has occured, Please try later!
            </div>
            <div class="error-actions">
                <a href="/" class="btn btn-primary btn-lg">
                    <span class="glyphicon glyphicon-home"></span>
                   Home
                </a>
            </div>
        </div>
    </div>
</div>

